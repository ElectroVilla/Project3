{
  "name": "udagram-api",
  "version": "2.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node .",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "prod": "tsc && node ./www/server.js",
    "clean": "rm -rf www/ || true",
    "cleanWin": "del -rf www/ || true",
    "cleanWin2": "RMDIR /S /Q ./www/",
    "build": "npm run clean && tsc && cp -rf src/config www/config && cp .npmrc www/.npmrc && cp package.json www/package.json && cd www && zip -r Archive.zip . && cd ..",
    "buildWin": "tsc && copy \"src/config\" \"www/config\" && copy \".npmrc\" \"www/.npmrc\" && copy \"package.json\" \"www/package.json\" && cd www && zip -r Archive.zip . && cd ..",
    "buildWin1": "tsc && copy \"src/config\" \"www/config\" && copy \".npmrc\" \"www/.npmrc\" && copy \"package.json\" \"www/package.json\"",
    "deployOld": "eb deploy --profile default",
    "init": "eb init $AWS_EB_APP --platform node.js --region us-east-1",
    "create": "eb create --sample $AWS_EB_ENV",
    "use": "eb use $AWS_EB_ENV",
    "setenv": "chmod +x bin/deploy.sh && bin/deploy.sh",
    "deployStep": "eb deploy $AWS_EB_ENV",
    "deploy": "npm run init && npm run use && npm run setenv && npm run deployStep",
    "deploy2": "npm run build && eb deploy",
    "testOrg": "echo \"Error: no test specified\" && exit 1",
    "test": "echo \"Dummy Test Passed\""
  },
  "keywords": [],
  "author": "Gabriel Ruttner",
  "license": "ISC",
